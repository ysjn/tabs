{
  "name": "tabs",
  "version": "0.0.1",
  "description": "tab manager",
  "manifest_version": 2,
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_title": "tabs"
  },
  "background": {
    "scripts": ["browser-polyfill.min.js", "init.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
